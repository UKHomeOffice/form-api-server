{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-console": [
      true
    ],
    "quotemark": [
      true,
      "single"
    ],
    "only-arrow-functions": true,
    "no-any": [
      true,
      {
        "ignore-rest-args": true
      }
    ],
    "no-unused-variable": [true, {"check-parameters" :  true}]
  },
  "rulesDirectory": []
}
