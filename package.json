{
  "name": "form-api-server",
  "version": "0.0.1",
  "description": "Form API Server",
  "main": "index.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run serve",
    "serve": "node dist/bootstrap.js",
    "watch-node": "nodemon dist/bootstrap.js",
    "watch-ts": "tsc -w",
    "test": "npm run test:all",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "test:int": "mocha --file ./test/integration/setup-test.ts -r ts-node/register test/integration/**/*.spec.ts ",
    "test:unit": "mocha -r ts-node/register test/unit/**/*.spec.ts",
    "test:all": "npm run test:unit && npm run test:int",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:int": "nyc --silent --no-clean npm run test:int",
    "cover:report": "nyc report --reporter=lcov --reporter=text --reporter=text-summary",
    "cover": "npm run cover:unit && npm run cover:int && npm run cover:report",
    "cover:merge": "npm run cover:unit && npm run cover:int && nyc merge .nyc_output coverage.json"
  },
  "contributors": [
    {
      "name": "Amin Mohammed-Coleman",
      "email": "amin@digitalpatterns.io"
    },
    {
      "name": "Mark Olliver",
      "email": "mark@digitalpatterns.io"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && npm run tslint:fix && npm run cover"
    }
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text",
      ""
    ],
    "all": true,
    "extension": [
      ".ts"
    ],
    "sourceMap": true,
    "instrument": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "src/*.ts",
      "src/types/*.ts",
      "src/interfaces/*.ts",
      "src/constant/*.ts",
      "src/error/*.ts",
      "src/formio/*.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalPatterns/form-api-server.git"
  },
  "keywords": [
    "form"
  ],
  "author": "Amin Mohammed-Coleman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DigitalPatterns/form-api-server/issues"
  },
  "homepage": "https://github.com/DigitalPatterns/form-api-server#readme",
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.89.0",
    "@types/chai": "^4.1.7",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/hapi__joi": "^15.0.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "husky": "^2.4.1",
    "lint": "^0.7.0",
    "mocha": "^6.1.4",
    "node-mocks-http": "^1.7.6",
    "nyc": "^14.1.1",
    "sqlite3": "^4.0.9",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/keycloak-connect": "^4.5.1",
    "@types/lodash": "^4.14.134",
    "@types/lru-cache": "^5.1.0",
    "@types/morgan": "^1.7.35",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.1",
    "@types/winston": "^2.4.4",
    "@types/xregexp": "^3.0.30",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.2",
    "formiojs": "^3.22.10",
    "http-status-codes": "^1.3.2",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-express-utils": "^6.3.2",
    "keycloak-admin": "^1.12.0",
    "keycloak-connect": "^6.0.1",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "lru-cache": "^5.1.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pg-types": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.8.12",
    "sequelize-cli": "^5.5.0",
    "sequelize-querystring": "^0.10.0",
    "sequelize-typescript": "^1.0.0-beta.3",
    "swagger-express-ts": "^1.0.1",
    "swagger-ui-dist": "^3.22.3",
    "type-cacheable": "^0.1.9",
    "uninstall": "0.0.0",
    "uuid": "^3.3.2",
    "validator": "^11.0.0",
    "winston": "^3.2.1",
    "xregexp": "^4.2.4"
  }
}
